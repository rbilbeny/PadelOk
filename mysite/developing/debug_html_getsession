        function hayTerminosContratacion() {

            var rutaFichero = "https://padelestorilcl.matchpoint.com.es/files.ashx?id=5ae277abb1e697e13e0fa0082192ac6b";
            if (rutaFichero.length > 0)
                return true;
            else
                return false;
        }

        function terminosContratacion() {
            var rutaFichero = "https://padelestorilcl.matchpoint.com.es/files.ashx?id=5ae277abb1e697e13e0fa0082192ac6b";

            try {
                if (rutaFichero.length > 0) {
                    $('<iframe id="idContratacion" style="width: 100%!important;height: 100%!important;">').dialog({
                        dialogClass: "infoNormativa"
                    })
                    .attr('src', rutaFichero);
                    styles();
                }

                $("#idContratacion")[0].style.height = "100%";
                $("#idContratacion")[0].style.width = "100%";

            } catch (e) {
            }
        }
    </script>
